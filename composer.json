{
    "name": "php-etl/harbor-api-client",
    "description": "This package provides Jane-PHP generated API models and client based on the OpenAPI specification for Harbor.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Gyroscops\\Harbor\\Api\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Kiboko SAS",
            "homepage": "http://kiboko.fr"
        },
        {
            "name": "Gr√©gory Planchat",
            "email": "gregory@kiboko.fr"
        }
    ],
    "prefer-stable": true,
    "require": {
        "php": "^8.0",
        "jane-php/open-api-runtime": "^7.8"
    },
    "require-dev": {
        "jane-php/open-api-2": "^7.8",
        "friendsofphp/php-cs-fixer": "^3.13"
    },
    "config": {
        "bin-dir": "bin",
        "allow-plugins": {
            "php-http/discovery": false
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.3.x-dev"
        }
    }
}
